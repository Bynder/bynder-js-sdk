{
  "name": "@bynder/bynder-js-sdk",
  "version": "3.0.0",
  "description": "Bynder Javascript SDK",
  "main": "dist/bynder-js-sdk.cjs.js",
  "module": "dist/bynder-js-sdk.esm.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run build -- -w",
    "test": "jest --coverage",
    "lint": "eslint --config .eslintrc src/ tests/",
    "test:watch": "npm run test -- --watch src/ test/ --notify",
    "pretest": "npm run lint",
    "prepublish": "npm run build && npm run docs",
    "docs": "jsdoc --destination docs --readme README.md src/**/*.js"
  },
  "author": "Bynder",
  "license": "MIT",
  "files": [
    "dist/bynder-js-sdk.cjs.js",
    "dist/bynder-js-sdk.esm.js"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.6",
    "rollup": "^2.28.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "is-url": "^1.2.4",
    "query-string": "^6.1.0",
    "simple-oauth2": "^2.5.1",
    "uuid": "^8.3.0"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  },
  "repository": "git@github.com:Bynder/bynder-js-sdk.git",
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/docs/**"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/resources/",
      "/repl.js",
      "/coverage/",
      "/samples/",
      "/docs/",
      "/tests/helpers/",
      "/rollup.config.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}
